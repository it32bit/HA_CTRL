{
    "version": 4,
    "configurations": [
        {
            "name": "STM32",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "gcc-arm",
            "includePath": [
                "${workspaceRoot}/**",
                "${workspaceRoot}/Platform/Interface",
                "${workspaceRoot}/Platform/Interface/PilGpio",
                "${workspaceRoot}/Platform/STM32F4/Inc",
                "${workspaceRoot}/Platform/STM32F4/Src",
                "${workspaceRoot}/Core/Inc",
                "${workspaceRoot}/Core/Src",
                "${workspaceRoot}/App/Inc",
                "${workspaceRoot}/App/Src",
                "${workspaceRoot}/Bootloader/Inc",
                "${workspaceRoot}/Bootloader/Src",
                "${workspaceRoot}/Drivers/stm32f4xx-hal-driver/Inc",
                "${workspaceRoot}/Drivers/stm32f4xx-hal-driver/Src",
                "${workspaceRoot}/Drivers/cmsis-device-f4/Include",
                "${workspaceRoot}/Drivers/CMSIS/Core/Include"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "defines": [
                "DEBUG",
                "STM32F407xx",
                "USE_HAL_DRIVER",
                "USE_FULL_LL_DRIVER",
                "__cplusplus=202002L"
            ],
            "compileCommands": "${workspaceFolder}/build/debug/compile_commands.json"
        }
    ]
}